": "", "segments": null, "site_id": null, "site_type": null, "solrQuery": "", "sortBy": null, "sortOrder": null, "startPage": null, "style": null, "suggested": null, "target": null, "template": "resource/base", "timestamp": 1732548516.2449224, "tz_aware": null, "unionFacets": "", "webcast_permalink": null}, "page": [{"height": 0, "levels": 1, "mimetype": "image/tiff", "size": 163139144, "url": "https://tile.loc.gov/storage-services/master/pnp/jpd/00500/00507u.tif", "width": 0}, {"height": 103, "levels": 1, "mimetype": "image/jpeg", "size": 4262, "url": "https://tile.loc.gov/storage-services/service/pnp/jpd/00500/00507_150px.jpg", "width": 150}, {"height": 102, "levels": 1, "mimetype": "image/gif", "size": 12689, "url": "https://tile.loc.gov/storage-services/service/pnp/jpd/00500/00507t.gif", "width": 150}, {"height": 437, "levels": 1, "mimetype": "image/jpeg", "size": 49306, "url": "https://tile.loc.gov/storage-services/service/pnp/jpd/00500/00507r.jpg", "width": 640}, {"height": 700, "levels": 1, "mimetype": "image/jpeg", "size": 138120, "url": "https://tile.loc.gov/storage-services/service/pnp/jpd/00500/00507v.jpg", "width": 1024}], "pagination": {"current": 1, "next": null, "previous": null, "total": 1}, "related": {"group_record": null, "lot_link": null, "neighbors": "//www.loc.gov/pictures/related/?&co=jpd&pk=2009615477&st=gallery&sb=call_number#focus", "survey_link": null}, "related_items": [{"access_restricted": false, "aka": ["https://hdl.loc.gov/loc.wdl/wdl.17828", "http://www.loc.gov/resource/wdl.17828/", "http://lccn.loc.gov/2021666490", "http://www.loc.gov/resource/gdcwdl.wdl_17828/", "http://www.loc.gov/item/2021666490/"], "campaigns": [], "contributor": ["kitao, masayoshi"], "date": "1790-01-01", "dates": ["1790"], "description": ["Raikin zui (Pictures of imported birds, also seen as Pictures of birds imported from abroad) is a color-printed picture book that depicts birds that were shipped to Japan from overseas during the Edo period. The pictures were painted by the Ukiyo-e artist Kitao Masayoshi (Kuwagata Keisai, 1764-1824). The book was published in 1790. During the Tenmei (1781-89) and Kansei (1789-1801) periods, many sumptuously detailed color-printed picture books were published in Japan. They used advanced and time-consuming printing techniques, such as kirazuri (application of metallic pigments and mica), karazuri (blind-printing), and bokashi (graduation of color). This book is considered to be the finest example of such books. It consists of two pictures of the Chinese man from Nanking who supplied the rare birds from which the illustrations were made, and ten pictures that combine birds and plants in skillful compositions. This version in the National Diet Library is missing one picture of the Chinese supplier, the introduction, explanatory notes, and the colophon. Many picture books depicting plants and animals were made during the Edo period, the result of a concurrence of the deepening of honzo\u0304gaku (traditional pharmacognosy--the knowledge of medical substances of biological origin, particularly from plants) from China and the arrival of knowledge of Western natural history from Holland. This work was intended not only to be enjoyed as a beautiful picture book; it also is thought to have fulfilled the role, as a document that accurately depicted rare birds, of satisfying the growing popular interest in natural history."], "digitized": true, "extract_timestamp": "2021-07-23T21:23:39.166Z", "group": ["wdlpartner", "catalog-split-04", "catalog", "world-digital-library-partners", "main-catalog-split-04", "main-catalog"], "hassegments": true, "id": "http://www.loc.gov/item/2021666490/", "image_url": ["https://tile.loc.gov/image-services/iiif/service:gdc:gdcwdl:wd:l_:17:82:8:wdl_17828:0015/full/pct:6.25/0/default.jpg", "https://tile.loc.gov/image-services/iiif/service:gdc:gdcwdl:wd:l_:17:82:8:wdl_17828:0015/full/pct:6.25/0/default.jpg#h=285&w=416", "https://tile.loc.gov/image-services/iiif/service:gdc:gdcwdl:wd:l_:17:82:8:wdl_17828:0015/full/pct:12.5/0/default.jpg#h=571&w=833", "https://tile.loc.gov/image-services/iiif/service:gdc:gdcwdl:wd:l_:17:82:8:wdl_17828:0015/full/pct:25/0/default.jpg#h=1143&w=1666", "https://tile.loc.gov/image-services/iiif/service:gdc:gdcwdl:wd:l_:17:82:8:wdl_17828:0015/full/pct:50/0/default.jpg#h=2287&w=3332", "https://tile.loc.gov/image-services/iiif/service:gdc:gdcwdl:wd:l_:17:82:8:wdl_17828:0015/full/pct:100/0/default.jpg#h=4574&w=6664"], "index": 1, "language": ["japanese"], "location": ["japan"], "mime_type": ["application/zip", "application/pdf", "image/jpeg", "image/jp2"], "number": ["2021666490", "2021-05-26t13:43:12"], "online_format": ["compressed data", "pdf", "image"], "original_format": ["book"], "partof": ["national diet library", "world digital library - partner items", "catalog", "world digital library"], "score": 27.378803, "shelf_id": "http://lccn.loc.gov/2021666490", "site": ["catalog"], "subject": ["woodcuts", "block printing", "ukiyo-e", "japan", "edo period", "flowers", "birds", "plants"], "timestamp": "2022-09-11T21:10:14.463Z", "title": "Pictures of Imported Birds.", "type": ["text"], "url": "https://www.loc.gov/item/2021666490/"}, {"access_restricted": false, "aka": ["http://www.loc.gov/item/2021667437/", "http://www.loc.gov/resource/wdl.11833/", "http://www.loc.gov/resource/gdcwdl.wdl_11833/", "http://lccn.loc.gov/2021667437", "https://hdl.loc.gov/loc.wdl/wdl.11833"], "campaigns": [], "contributor": ["sakamoto, k\u014dnen"], "date": "1830-01-01", "dates": ["1830"], "description": ["The sakura (cherry blossom) is the most famous flower of Japan, the beauty of which has long inspired artists in Japan. Created in the middle of the 19th century, O\u0304ka-fu (A sketch book of cherry blossoms) contains the names and illustrations of 29 varieties of sakura, which are painted on silk with delicate brushwork. The artist, Sakamoto Ko\u0304nen (1800-53), studied herbal medicine under his father, Sakamoto Jun'an, physician-in-ordinary to the sovereign lord of Kishu\u0304 Domain (present-day Wakayama Prefecture). Sakamoto Ko\u0304nen also worked in Kishu\u0304 Domain as a physician. The richly bound book bears the ownership stamp of the Kishu\u0304 Tokugawa family and is presumed to have been dedicated to the lord of Kishu\u0304 by the artist."], "digitized": true, "extract_timestamp": "2021-07-24T02:05:54.417Z", "group": ["wdlpartner", "catalog-split-06", "catalog", "world-digital-library-partners", "main-catalog-split-06", "main-catalog"], "hassegments": true, "id": "http://www.loc.gov/item/2021667437/", "image_url": ["https://tile.loc.gov/image-services/iiif/service:gdc:gdcwdl:wd:l_:11:83:3:wdl_11833:1286920_R0000016/full/pct:12.5/0/default.jpg", "https://tile.loc.gov/image-services/iiif/service:gdc:gdcwdl:wd:l_:11:83:3:wdl_11833:1286920_R0000016/full/pct:12.5/0/default.jpg#h=389&w=511", "https://tile.loc.gov/image-services/iiif/service:gdc:gdcwdl:wd:l_:11:83:3:wdl_11833:1286920_R0000016/full/pct:25/0/default.jpg#h=779&w=1022", "https://tile.loc.gov/image-services/iiif/service:gdc:gdcwdl:wd:l_:11:83:3:wdl_11833:1286920_R0000016/full/pct:50/0/default.jpg#h=1559&w=2045", "https://tile.loc.gov/image-services/iiif/service:gdc:gdcwdl:wd:l_:11:83:3:wdl_11833:1286920_R0000016/full/pct:100/0/default.jpg#h=3118&w=4090"], "index": 2, "language": ["japanese"], "location": ["japan"], "mime_type": ["application/zip", "application/pdf", "image/jpeg", "image/jp2"], "number": ["2021667437", "2021-05-27t17:59:43"], "online_format": ["compressed data", "pdf", "image"], "original_format": ["book"], "partof": ["national diet library", "world digital library - partner items", "catalog", "world digital library"], "score": 21.877045, "shelf_id": "http://lccn.loc.gov/2021667437", "site": ["catalog"], "subject": ["cherry trees", "1830 to 1853", "japan", "flowers"], "timestamp": "2022-09-11T21:29:44.655Z", "title": "A Sketch Book of Sakura (Cherry Blossoms).", "type": ["text"], "url": "https://www.loc.gov/item/2021667437/"}, {"access_restricted": false, "aka": ["http://www.loc.gov/resource/gdcwdl.wdl_11832/", "http://lccn.loc.gov/2021666418", "http://www.loc.gov/resource/wdl.11832/", "http://www.loc.gov/item/2021666418/", "https://hdl.loc.gov/loc.wdl/wdl.11832"], "campaigns": [], "contributor": ["kitagawa, utamaro"], "date": "1788-01-01", "dates": ["1788"], "description": ["Ehon mushi-erami (The insect book) is by the ukiyo-e painter Kitagawa Utamaro (circa 1753-1806). It was created by him before he produced the bijin-ga (pictures of beautiful women) for which he is famous. Each double page of the book contains a painted illustration of a plant and two species of insects, along with two kyo\u0304ka (a poem style originating from waka, literally, Japanese poems). The kyo\u0304ka are ostensibly insect-themed love poems. In all, 15 colored wood-block prints are included. The work demonstrates Utamaro's skill at drawing, as well as the high-quality printing techniques of the day. The book was published by Tsutaya Juzaburo\u0304 (1750-97), a press mogul in the late Edo period (1600-1868)."], "digitized": true, "extract_timestamp": "2021-07-23T21:09:08.776Z", "group": ["wdlpartner", "catalog-split-04", "catalog", "world-digital-library-partners", "main-catalog-split-04", "main-catalog"], "hassegments": true, "id": "http://www.loc.gov/item/2021666418/", "image_url": ["https://tile.loc.gov/image-services/iiif/service:gdc:gdcwdl:wd:l_:11:83:2:wdl_11832:1288345_R0000007/full/pct:12.5/0/default.jpg", "https://tile.loc.gov/image-services/iiif/service:gdc:gdcwdl:wd:l_:11:83:2:wdl_11832:1288345_R0000007/full/pct:12.5/0/default.jpg#h=403&w=512", "https://tile.loc.gov/image-services/iiif/service:gdc:gdcwdl:wd:l_:11:83:2:wdl_11832:1288345_R0000007/full/pct:25/0/default.jpg#h=807&w=1024", "https://tile.loc.gov/image-services/iiif/service:gdc:gdcwdl:wd:l_:11:83:2:wdl_11832:1288345_R0000007/full/pct:50/0/default.jpg#h=1614&w=2049", "https://tile.loc.gov/image-services/iiif/service:gdc:gdcwdl:wd:l_:11:83:2:wdl_11832:1288345_R0000007/full/pct:100/0/default.jpg#h=3228&w=4099"], "index": 3, "language": ["japanese"], "location": ["japan"], "mime_type": ["application/zip", "application/pdf", "image/jpeg", "image/jp2"], "number": ["2021666418", "2021-05-26t11:44:28"], "online_format": ["compressed data", "pdf", "image"], "original_format": ["book"], "partof": ["national diet library", "world digital library - partner items", "catalog", "world digital library"], "score": 21.076021, "shelf_id": "http://lccn.loc.gov/2021666418", "site": ["catalog"], "subject": ["block printing", "ukiyo-e", "japan", "flowers", "japanese poetry", "insects", "plants", "poetry"], "timestamp": "2022-09-11T21:08:21.977Z", "title": "The Insect Book.", "type": ["text"], "url": "https://www.loc.gov/item/2021666418/"}, {"access_restricted": false, "aka": ["http://www.loc.gov/resource/wdl.2929/", "http://www.loc.gov/resource/gdcwdl.wdl_02929/", "http://www.loc.gov/item/2021666120/", "http://lccn.loc.gov/2021666120", "https://hdl.loc.gov/loc.wdl/wdl.2929"], "campaigns": [], "contributor": ["fujiwara, sadaie", "hishikawa, moronobu"], "date": "1680-01-01", "dates": ["1680"], "description": ["This illustrated book of Ogura hyakunin isshu (One hundred poets, one hundred poems) is a collection of one hundred 31-syllable classical Japanese poems (waka), each by a different poet. The collection is organized chronologically from Emperor Tenji (626-671) to Emperor Juntoku (1197-1242). Each of the poets is depicted by a woodblock print created by Hishikawa Moronobu (1618-circa 1694). Morobonu is often considered the first Ukiyo-e artist."], "digitized": true, "extract_timestamp": "2021-07-23T19:06:37.740Z", "group": ["wdlpartner", "catalog-split-10", "catalog", "world-digital-library-partners", "main-catalog-split-10", "main-catalog"], "hassegments": true, "id": "http://www.loc.gov/item/2021666120/", "image_url": ["https://tile.loc.gov/image-services/iiif/service:gdc:gdcwdl:wd:l_:02:92:9:wd